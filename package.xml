<?xml version="1.0"?>
<package format="2">
    <name>uav_octomapping</name>
    <version>2.1.0</version>
    <description>
        A ROS package to build octomap with LiDAR and D435i
    </description>
    <author email="eason473867143@gmail.com">Eason Yi</author>
    <maintainer email="hyx020222@gmail.com">Eason Hua</maintainer>
    <license>Apache</license>
    <url type="website">https://amovlab.com</url>

    <buildtool_depend>catkin</buildtool_depend>

    <depend>roscpp</depend>
    <depend>rospy</depend>
    <depend>std_msgs</depend>
    <depend>sensor_msgs</depend>
    <depend>geometry_msgs</depend>
    <depend>nav_msgs</depend>
    <depend>prometheus_msgs</depend>
    <depend>pcl_ros</depend>
    <depend>octomap</depend>
    <depend>message_filters</depend>
    <depend>tf</depend>

    <export>
        <!-- Other tools can request additional information be placed here -->
        <build_type>catkin</build_type>
    </export>
</package>
